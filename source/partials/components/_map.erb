<% embed_url ||= '' %>
<% heading ||= '' %>
<% address ||= '' %>
<% contact ||= '' %>

<section class="map">

  <iframe class="map__map" width='100%' height='500px' frameBorder='0' src='<%= embed_url %>'></iframe>

  <div class="map__content">
    <h5 class="map__heading"><%= heading %></h5>
    <div class="map__address">
      <p>
      <%= address %>
      </p>
    </div>
    <div class="map__contact">
      <p>
      <%= contact %>
      </p>
    </div>
  </div>

</section>
